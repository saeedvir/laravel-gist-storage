{
    "name": "saeedvir/laravel-gist-storage",
    "description": "Laravel Storage driver for GitHub Gist - Store files on GitHub Gist using Laravel's Storage facade",
    "type": "library",
    "keywords": [
        "laravel",
        "storage",
        "gist",
        "github",
        "flysystem",
        "filesystem",
        "livewire"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Saeedvir",
            "email": "saeed.es91@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/filesystem": "^11.0|^12.0",
        "league/flysystem": "^3.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Saeedvir\\LaravelGistStorage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saeedvir\\LaravelGistStorage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saeedvir\\LaravelGistStorage\\GistStorageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}